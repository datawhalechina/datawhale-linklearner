{
  "name": "datawhale-homepage",
  "description": "datawhale homepage",
  "scripts": {
    "build:client": "rm -rf output/client && webpack --config ./package/client/build/webpack.prod.js",
    "build:server": "NODE_ENV=prod && rm -rf output/* && tsc --p ./package/server",
    "clean": "rm -rf output",
    "dev:client": "webpack serve --progress --hot --config ./package/client/build/webpack.dev.js",
    "dev:server": "NODE_ENV=dev && nodemon --watch './package/server/**/*.ts' --exec 'ts-node' ./package/server/app.ts"
  },
  "author": "shadowings-zy",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/express": "^4.17.13",
    "@vue/compiler-sfc": "^3.2.23",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "nodemon": "^2.0.15",
    "postcss-loader": "^6.2.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "vue": "^3.2.23",
    "vue-loader": "^16.8.3",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41"
  }
}
